#include <stdio.h>
int main() {
    int arr[] = {7, 9, 12, 6, 5, 4};
    int n = sizeof(arr) / sizeof(arr[0]);
    int target = 8, i, found = 0;
    
    for(i = 0; i < n; i++) {
        if(arr[i] == target) {
            printf("Element %d found at position %d\n", target, i + 1);
            found = 1;
            break;
        }
    }

